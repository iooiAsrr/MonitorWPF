<Page x:Class="MonitorWPF.Views.SettingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MonitorWPF.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SettingPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1000"></RowDefinition>
            <RowDefinition Height="800"></RowDefinition>
            <RowDefinition Height="800"></RowDefinition>
            <RowDefinition Height="800"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="设备清单" x:Name="data1" Grid.Row="0" VerticalContentAlignment="Top"/>
        <Label Content="环境监测" x:Name="data2" Grid.Row="1" VerticalAlignment="Top"/>
        <Label Content="设备监测" x:Name="data3"  Grid.Row="2" VerticalAlignment="Top"/>
        <Label Content="配置一" x:Name="data4"  Grid.Row="3" VerticalAlignment="Top"/>
    </Grid>
</Page>
